{
    "name": "codica-studio/adminmedialibrary",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/adminmedialibrary",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "laravel/nova": "^3.0",
        "spatie/laravel-medialibrary": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ebess\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ebess\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
